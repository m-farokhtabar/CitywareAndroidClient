<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="EditTextPersian">
        <attr name="IsRequired" format="boolean"/>
        <attr name="Title" format="string"/>
        <attr name="ValidationType" format="enum">
            <enum name="None" value="0"/>
            <enum name="Email" value="1"/>
            <enum name="WebSite" value="2"/>
            <enum name="CellPhoneNumber" value="3"/>
            <enum name="TrackingCode" value="4"/>
            <enum name="PostalCode" value="5"/>
            <enum name="PersianDate" value="6"/>
            <enum name="AccountNumber" value="7"/>
            <enum name="PersianTimeRegular" value="8"/>
            <enum name="PhoneRegular" value="9"/>
            <enum name="IntegerNumber" value="10"/>
            <enum name="FloatNumber" value="11"/>
            <enum name="FloatPercent" value="12"/>
        </attr>
    </declare-styleable>
</resources>